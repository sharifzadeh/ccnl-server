<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        >
    <class
            name="edu.dartmouth.ccnl.ridmp.dto.ShapeTO"
            table="shape"
            dynamic-update="true"
            dynamic-insert="true"
            >

        <id
                name="shapeId"
                type="java.lang.Integer"
                column="shapeId"
                >

            <generator class="increment">
            </generator>
        </id>


        <property
                name="size"
                type="java.lang.Integer"
                update="true"
                insert="true"
                column="size"
                />

        <property
                name="std"
                type="java.lang.Double"
                update="true"
                insert="true"
                column="std"
                />
        <property
                name="phase"
                type="java.lang.Double"
                update="true"
                insert="true"
                column="phase"
                />

        <property
                name="frequencies"
                type="java.lang.String"
                update="true"
                insert="true"
                column="frequencies"
                />
        <property
                name="orientations"
                type="java.lang.String"
                update="true"
                insert="true"
                column="orientations"
                />

        <property
                name="env"
                type="java.lang.String"
                update="true"
                insert="true"
                column="env"
                />

        <property
                name="contrast"
                type="java.lang.String"
                update="true"
                insert="true"
                column="contrast"
                />

        <property
                name="bgColor"
                type="java.lang.Double"
                update="true"
                insert="true"
                column="bgColor"
                />
    </class>

</hibernate-mapping>